<?xml version="1.0" encoding="utf-8" ?>

<NEKTAR xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="http://www.nektar.info/schema/nektar.xsd">

    <EXPANSIONS>
        <E COMPOSITE="C[0]" NUMMODES="$NUMMODES" FIELDS="u,v,w,p" TYPE="MODIFIED" />
    </EXPANSIONS>

    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="SolverType" VALUE="VelocityCorrectionScheme" />
            <I PROPERTY="EQTYPE" VALUE="UnsteadyNavierStokes" />
            <I PROPERTY="AdvectionForm" VALUE="Convective" />
            <I PROPERTY="Projection" VALUE="Galerkin" />
            <I PROPERTY="TimeIntegrationMethod" VALUE="IMEXOrder2" />
        </SOLVERINFO>

        <PARAMETERS>
            <P> TimeStep      = $TIMESTEP            </P>
            <P> NumSteps      = $NUMSTEPS            </P>
            <P> IO_CheckSteps = $IO_CHECKSTEPS       </P>
            <P> IO_InfoSteps  = $IO_INFOSTEPS        </P>
            <P> Kinvis        = $KINVIS              </P>
        </PARAMETERS>

        <VARIABLES>
            <V ID="0"> u </V>
            <V ID="1"> v </V>
            <V ID="2"> w </V>
            <V ID="3"> p </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1] </B>  <!-- Inflow -->
            <B ID="1"> C[2] </B>  <!-- Outflow -->
            <B ID="2"> C[3] </B>  <!-- side wall -->
            <B ID="3"> C[4] </B>  <!-- side wall -->
            <B ID="4"> C[5] </B>  <!-- top wall -->
            <B ID="5"> C[6] </B>  <!-- bottom wall -->
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">  <!-- inflow -->
                <D VAR="u" VALUE="(1-exp(-0.963740544195769*x)*cos(2*PI*y))" />
                <D VAR="v" VALUE="(-0.963740544195769/(2*PI))*exp(-0.963740544195769*x)*sin(2*PI*y)" />
                <D VAR="w" VALUE="0" />
                <N VAR="p" USERDEFINEDTYPE="H" VALUE="0" />
            </REGION>
            <REGION REF="1">  <!-- outflow -->
                <D VAR="u" VALUE="(1-exp(-0.963740544195769*x)*cos(2*PI*y))" />
                <D VAR="v" VALUE="(-0.963740544195769/(2*PI))*exp(-0.963740544195769*x)*sin(2*PI*y)" />
                <D VAR="w" VALUE="0" />
                <D VAR="p" VALUE="0.5*(1-exp(-2*0.963740544195769*x))" />
            </REGION>
            <REGION REF="2">  <!-- side wall -->
                <N VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="w" VALUE="0" />
                <N VAR="p" VALUE="0" />
            </REGION>
            <REGION REF="3">  <!-- side wall -->
                <N VAR="u" VALUE="0" />
                <D VAR="v" VALUE="0" />
                <D VAR="w" VALUE="0" />
                <N VAR="p" VALUE="0" />
            </REGION>
            <REGION REF="4">  <!-- top -->
                <D VAR="u" VALUE="(1-exp(-0.963740544195769*x)*cos(2*PI*y))" />
                <D VAR="v" VALUE="(-0.963740544195769/(2*PI))*exp(-0.963740544195769*x)*sin(2*PI*y)" />
                <D VAR="w" VALUE="0" />
                <D VAR="p" VALUE="0.5*(1-exp(-2*0.963740544195769*x))" />
            </REGION>
            <REGION REF="5">  <!--bottom -->
                <D VAR="u" VALUE="(1-exp(-0.963740544195769*x)*cos(2*PI*y))" />
                <D VAR="v" VALUE="(-0.963740544195769/(2*PI))*exp(-0.963740544195769*x)*sin(2*PI*y)" />
                <D VAR="w" VALUE="0" />
                <D VAR="p" VALUE="0.5*(1-exp(-2*0.963740544195769*x))" />
            </REGION>
        </BOUNDARYCONDITIONS>

        <FUNCTION NAME="InitialConditions">
            <E VAR="u" VALUE="0" />
            <E VAR="v" VALUE="0" />
            <E VAR="w" VALUE="0" />
            <E VAR="p" VALUE="0" />
        </FUNCTION>

        <FUNCTION NAME="ExactSolution">
            <E VAR="u" VALUE="(1-exp(-0.963740544195769*x)*cos(2*PI*y))" />
            <E VAR="v" VALUE="(-0.963740544195769/(2*PI))*exp(-0.963740544195769*x)*sin(2*PI*y)" />
            <E VAR="w" VALUE="0" />
            <E VAR="p" VALUE="0.5*(1-exp(-2*0.963740544195769*x))" />
        </FUNCTION>

    </CONDITIONS>

    <GEOMODEL FILENAME="$GEOMETRY">

        <COMPOSITE>
            <!-- Ignore the main region for now -->
            <!-- <C ID="0"> Q[0-11]         </C>  -->
            <!-- Geometric Faces for Boundary Regions -->
            <C ID="1"> F[104,112,114]  </C>  
            <C ID="2"> F[108]          </C>  
            <C ID="3"> F[110]          </C>  
            <C ID="4"> F[106]          </C>  
            <C ID="5"> F[34]           </C>  
            <C ID="6"> F[56]           </C>  
        </COMPOSITE>

    </GEOMODEL>

    <PUMI FILENAME="$PUMIMESH" ORDER="$ORDER">
    </PUMI>

    <GEOMETRY DIM="3" SPACE="3">

        <DOMAIN> C[0] </DOMAIN>

    </GEOMETRY>

</NEKTAR>
